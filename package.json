{
  "name": "pwtgen",
  "version": "2.0.22",
  "description": "AI-Powered Playwright Test Generator CLI - Modernized",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "pwtgen": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "embed": "tsx scripts/embed-knowledge-base.ts",
    "generate": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' src/cli.ts generate",
    "fresh-build": "rm -rf dist && npm uninstall -g pwtgen && npm run build && npm link"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.1",
    "ajv": "^8.17.1",
    "axios": "^1.6.0",
    "chalk": "^5.4.1",
    "clipboardy": "^4.0.0",
    "commander": "^11.1.0",
    "cosine-similarity": "^1.0.1",
    "dotenv": "^16.6.1",
    "inquirer": "^9.3.7",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.4",
    "natural": "^8.1.0",
    "openai": "^4.28.0",
    "ora": "^7.0.1",
    "prettier": "^3.1.0",
    "uri-js": "^4.4.1",
    "vectra": "^0.11.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/clipboardy": "^1.1.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "globals": "^16.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
